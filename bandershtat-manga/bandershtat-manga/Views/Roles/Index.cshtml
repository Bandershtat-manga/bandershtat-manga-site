@model List<DataAcess.Entity.Roles>

<h1 style="color: white">Список ролей</h1>

<table class="table" style="color: white">
    <thead>
        <tr>
            <th>Id</th>
            <th>Назва</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>
